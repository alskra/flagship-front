extends /pages/index/index

include /components/block-heading/block-heading

append variables
  -
    aside = false;

block main
  .grid-12.my-auto
    .col-12.col-8-min-md.col-start-3-min-md.col-6-min-lg.col-start-4-min-lg.col-4-min-xl.col-start-5-min-xl
      +block-heading('Регистрация').block-heading--sign-up
        div(x-text="$store.date")
        div(x-text="$store.formatTime($store.date, true)")
        div(x-text="$store.formatTime($store.date)")
        div(x-data="{ time: Math.floor(Date.now() / 1000) + 5 * 60 }" x-text='$store.formatTime(time - $store.date)')
